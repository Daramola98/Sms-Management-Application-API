    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "nodemon": "^1.18.4"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
